<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RecentActivity"
            android:label="@string/RecentActivity" />
        <activity
            android:name=".TopChartsActivity"
            android:label="@string/TopChartsActivity" />
        <activity
            android:name=".RecommendedActivity"
            android:label="@string/RecommendedActivity" />
        <activity
            android:name=".FeelingLuckyActivity"
            android:label="@string/FeelingLuckyActivity" />
        <activity
            android:name=".MyMusicActivity"
            android:label="My Music"/>
        <activity android:name=".MusicPlayingActivity"></activity>
    </application>

</manifest>